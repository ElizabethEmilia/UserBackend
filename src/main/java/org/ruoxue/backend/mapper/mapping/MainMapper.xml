<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.ruoxue.backend.mapper.MainMapper">

    <select id="getTAdminByName" resultType="org.ruoxue.backend.bean.TAdmin">
        select * from t_admin where name = #{name}
    </select>

    <select id="getTCustomerByName" resultType="org.ruoxue.backend.bean.TCustomer">
        select * from t_customer where name = #{name}
    </select>


</mapper>
